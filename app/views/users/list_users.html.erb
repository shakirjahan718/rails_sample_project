<h1>Users List</h1>
<table style= "border: 2px solid">
  <tr>
    <th>Name</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>City</th>
    <th>Age</th>
    <th>Gender</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name%></td>
      <td><%= link_to "#{user.firstname}", show_user_path(id: user) %></td>
      <td><%= link_to "#{user.lastname}", show_user_path(id: user) %></td>
      <td><%= user.city_name%></td>
      <td><%= user.age %></td>
      <td><%= gender_string(user.gender) %></td>
    </tr>  
  <% end %>
</table>
